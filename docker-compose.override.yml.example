# 本地/生产环境变量覆盖文件
# 复制此文件为 docker-compose.override.yml 并设置你的配置
# docker-compose.override.yml 不会被提交到 Git

services:
  database:
    environment:
      POSTGRES_PASSWORD: ${DB_PASSWORD:-poster_password}

  backend:
    environment:
      DB_PASSWORD: ${DB_PASSWORD:-poster_password}
      JWT_SECRET: ${JWT_SECRET:-change-me-in-production-min-32-chars}
      FRONTEND_URL: ${FRONTEND_URL:-http://localhost:3000}

  algorithm:
    environment:
      LLM_PROVIDER: dashscope
      LLM_API_KEY: ${LLM_API_KEY:-}
      LLM_MODEL: qwen-turbo
      LLM_BASE_URL: ${LLM_BASE_URL:-}
      BAIDU_SECRET_KEY: ""
